{
  "name": "empatho-infrastructure",
  "version": "0.1.0",
  "bin": {
    "organizations": "bin/organizations.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "pretest": "npm run build",
