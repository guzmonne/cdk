    "@types/aws-lambda": "^8.10.82",
    "aws-cdk": "1.118.0",
    "aws-sdk": "^2.970.0",
    "aws-sdk-mock": "^5.2.2",
    "jest": "^26.6.3",
    "sinon": "^11.1.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.118.0",
    "@aws-cdk/aws-codecommit": "^1.118.0",
